{
  "name": "jjdyo/j-laravel-media-manager",
  "description": "A media manager package for Laravel with Vue.js frontend",
  "type": "library",
  "license": "MIT",
  "keywords": ["laravel", "media", "manager", "upload", "vue", "image"],
  "authors": [
    {
      "name": "Jaxon Dupre",
      "email": "jaxon.dupre@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "laravel/framework": "^10.0|^11.0|^12.0",
    "intervention/image": "^2.7|^3.0"
  },
  "suggest": {
    "spatie/laravel-permission": "Required if you want to use role-based access control for media"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Jjdyo\\MediaManager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jjdyo\\MediaManager\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jjdyo\\MediaManager\\MediaManagerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}